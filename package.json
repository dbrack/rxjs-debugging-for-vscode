{
  "name": "rxjs-debugging-for-vs-code",
  "displayName": "RxJS Debugging for VS Code",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "manuelalabor",
  "activationEvents": [
    "onCommand:rxjs-debugging-for-vs-code.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rxjs-debugging-for-vs-code.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "test": "yarn test:integration",
    "pretest:integration": "yarn run compile",
    "test:integration": "node ./out/integrationTest/runTest.js",
    "pretty": "prettier --write",
    "package": "vsce package --yarn"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "husky": "^5.1.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.1.3",
    "prettier": "^2.2.1",
    "typescript": "^4.0.2",
    "vsce": "^1.85.0",
    "vscode-test": "^1.4.0"
  },
  "lint-staged": {
    "*.{ts,js,json}": [
      "yarn pretty"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
